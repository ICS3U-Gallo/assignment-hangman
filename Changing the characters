get_secret_word = "Apple"
guesses = input()
word = ""

if guesses == "a":
  word += "a"
  word += " _ "
  word += " _ "
  word += " _ "
  word += " _ "
  word += " _ "
  print(word)
elif guesses == "p":
  word += "_ p p _ _"
  print(word)
elif guesses == "l":
  word += "_ _ _ l _"
  print("word")
elif guesses == "e":
  word += "_ _ _ _ e"
  print("word")
else: 
  print("try again")
  print("_ _ _ _ _")
