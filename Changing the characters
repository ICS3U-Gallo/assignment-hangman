def underscr(word, guess):
  word_bank = list(word)
  guess = list(set(guess))
  charac = list(word)
  lengguess = len(guess)
  lengword = len(word)
  for l in range(0, lengword):
   if(charac[l] != " "):
    charac[l] = " _"
   for a in range(0, lengguess):
    for l in range(0, word.count(guess[a])):
     if(guess[a] in word):
      und = word_bank.index(guess[a])
      word_bank[und] = " _"
      charac[und] = guess[a]
   return " ".join(charac)
